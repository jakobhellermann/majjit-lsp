	},
	"engines": {
		"vscode": "^1.66.0"
	},
	"enabledApiProposals": [],
	"activationEvents": [
		"onLanguage:jjmagit"
	],
	"main": "./out/main.js",
	"contributes": {
		"languages": [
			{
				"id": "jjmagit",
				"extensions": [
					".jjmagit"
				]
			}
		],
		"commands": [
			{
				"command": "jjmagit.open.status",
				"title": "jjmagit: Status"
			}
		],
		"configuration": {
			"type": "object",
			"title": "jjmagit-language-server",
			"properties": {
				"jjmagit-language-server.trace.server": {
					"type": "string",
					"scope": "window",
					"enum": [
						"off",
						"messages",
						"verbose"
					],
					"enumDescriptions": [
						"No traces",
						"Error only",
						"Full log"
					],
					"default": "off",
					"description": "Traces the communication between VS Code and the language server."
				}
			}
		},
		"keybindings": [
			{
				"key": "tab",
				"command": "editor.toggleFold",
				"when": "editorLangId == 'jjmagit'"
			},
			{
				"key": "enter",
				"command": "editor.action.revealDefinition",
				"when": "editorLangId == 'jjmagit'"
			}
		],
